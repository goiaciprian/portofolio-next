generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum SkillType {
  OTHER
  MAIN
}

model Skill {
  id   String    @id @default(uuid())
  name String    @unique
  type SkillType

  @@map("skill")
}

model Project {
  id String @id @default(uuid())

  left  Json?
  right Json?

  @@map("project")
}
